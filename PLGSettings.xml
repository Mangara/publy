<?xml version="1.0" encoding="ISO-8859-1"?>
<plgsettings>
  <!-- Relative path to a BibTeX file with the publication list -->
  <publications></publications>

  <!-- Settings for plain-text output -->
  <generateplaintext>false</generateplaintext>
  <plaintextsettings>
    <target></target>
    <listallauthors>true</listallauthors>
    <presentedtext></presentedtext>
    <numbering>NONE</numbering>
    <categories>
      <category>CONFERENCE</category>
      <category>JOURNAL</category>
    </categories>
    <categorynotes>
      <note category="CONFERENCE">Co</note>
      <note category="JOURNAL">Jou</note>
    </categorynotes>
  </plaintextsettings>

  <!-- Settings for HTML output -->
  <generatehtml>false</generatehtml>
  <htmlsettings>
    <target></target>
    <listallauthors>true</listallauthors>
    <presentedtext></presentedtext>
    <numbering>NONE</numbering>
    <categories>
    </categories>
    <categorynotes>
    </categorynotes>
    <!-- HTML-specific settings -->
    <linktotextversion>true</linktotextversion>
    <includeabstract>ALL</includeabstract>
    <includebibtex>ARXIV</includebibtex>
    <includepdf>ARXIV</includepdf>
    <header></header>
    <footer></footer>
    <googleanalyticsuser></googleanalyticsuser>
  </htmlsettings>

</plgsettings>
